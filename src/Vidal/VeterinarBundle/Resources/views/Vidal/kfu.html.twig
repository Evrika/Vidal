{% extends 'VidalVeterinarBundle::vidal_layout.html.twig' %}


{% block nav %}
	<div class="breadcrumbs">
		<a href="{{ path('index') }}">Видаль</a>
		<i></i>
		<a href="{{ path('veterinar') }}">Видаль-Ветеринар</a>
		<i></i>
		{% if kfuName is defined %}
			<a href="{{ path('v_kfu') }}" title="Клинико-фармакологический указатель">КФУ</a>
			<i></i>
			<span>{{ kfuName }}</span>
		{% else %}
			<span title="Клинико-фармакологический указатель">КФУ</span>
		{% endif %}
	</div>
{% endblock %}


{% block vidal %}
	{% if menu_veterinar is defined %}
		{% include 'VidalVeterinarBundle:Vidal:veterinar_menu.html.twig' %}
	{% endif %}

{% endblock %}