{% extends 'VidalDrugBundle::vidal_layout.html.twig' %}


{% block title %}
	<h1>{{ kfu|raw }}</h1>
{% endblock %}


{% block nav %}
	<div class="breadcrumbs">
		<a href="{{ path('index') }}">Видаль</a>
		<i></i>
		<a href="{{ path('kfu') }}" title="Клинико-фармакологические указатели">КФУ</a>
		<i></i>
		<span>{{ kfu|raw }}</span>
	</div>
{% endblock %}


{% block vidal %}
	<div class="anons">Клинико-фармакологический указатель</div>

	{% if products is defined and products is not empty %}
		{% include 'VidalDrugBundle:Vidal:render_products.html.twig' %}
	{% else %}

	{% endif %}
{% endblock %}